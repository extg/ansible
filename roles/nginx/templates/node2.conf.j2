server {
    listen 80;

    server_name beta2.dafisha.ru www.beta2.dafisha.ru default;
    root /var/www/beta2.dafisha.ru/packages/dafisha/build;

    location / {
        proxy_pass http://167.99.110.129:8080;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection 'upgrade';
        proxy_set_header Host $host;
        proxy_cache_bypass $http_upgrade;
    }

    location ~ \.(js|css|svg)$ {
        #root /data/images;
    }
}
